<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>false</primitive>
  <delegateAnalysisEngineSpecifiers>
    <delegateAnalysisEngine key="PosTagAnnotator">
      <import location="PosTagAnnotator.xml"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="GeneAnnotator">
      <import location="GeneAnnotator.xml"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="SentenceAnnotator">
      <import location="SentenceAnnotator.xml"/>
    </delegateAnalysisEngine>
  </delegateAnalysisEngineSpecifiers>
  <analysisEngineMetaData>
    <name>aeDescriptor</name>
    <description/>
    <version>1.0</version>
    <vendor/>
    <configurationParameters searchStrategy="language_fallback"/>
    <configurationParameterSettings/>
    <flowConstraints>
      <fixedFlow>
        <node>SentenceAnnotator</node>
        <node>PosTagAnnotator</node>
        <node>GeneAnnotator</node>
      </fixedFlow>
    </flowConstraints>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs>
          <type>edu.cmu.hw1.types.Sentence</type>
          <type>edu.cmu.hw1.types.Noun</type>
          <type>edu.cmu.hw1.types.Genemarker</type>
          <feature>edu.cmu.hw1.types.Genemarker:geneName</feature>
          <feature>edu.cmu.hw1.types.Genemarker:geneId</feature>
          <feature>edu.cmu.hw1.types.Genemarker:end</feature>
          <feature>edu.cmu.hw1.types.Genemarker:begin</feature>
          <feature>edu.cmu.hw1.types.Noun:NounId</feature>
          <feature>edu.cmu.hw1.types.Noun:end</feature>
          <feature>edu.cmu.hw1.types.Noun:begin</feature>
          <feature>edu.cmu.hw1.types.Sentence:SentenceId</feature>
          <feature>edu.cmu.hw1.types.Sentence:end</feature>
          <feature>edu.cmu.hw1.types.Sentence:begin</feature>
        </inputs>
        <outputs>
          <type>edu.cmu.hw1.types.Noun</type>
          <type>edu.cmu.hw1.types.Genemarker</type>
          <feature>edu.cmu.hw1.types.Genemarker:geneName</feature>
          <feature>edu.cmu.hw1.types.Genemarker:geneId</feature>
          <feature>edu.cmu.hw1.types.Genemarker:end</feature>
          <feature>edu.cmu.hw1.types.Genemarker:begin</feature>
          <feature>edu.cmu.hw1.types.Noun:NounId</feature>
          <feature>edu.cmu.hw1.types.Noun:end</feature>
          <feature>edu.cmu.hw1.types.Noun:begin</feature>
        </outputs>
        <languagesSupported/>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
